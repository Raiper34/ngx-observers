{
  "name": "ngx-observers-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build ngx-observers --configuration production && copy-files-from-to",
    "build:docs": "npx compodoc -p tsconfig.doc.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:docs": "npm run build:docs -- -c package.json -s -w",
    "deploy": "npm run build:docs && netlify deploy --dir=documentation --prod",
    "publish": "npm run build:lib && cd dist/ngx-observers && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.4",
    "@angular/common": "^14.2.4",
    "@angular/compiler": "^14.2.4",
    "@angular/core": "^14.2.4",
    "@angular/forms": "^14.2.4",
    "@angular/platform-browser": "^14.2.4",
    "@angular/platform-browser-dynamic": "^14.2.4",
    "@angular/router": "^14.2.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.4",
    "@angular/cli": "^14.2.4",
    "@angular/compiler-cli": "^14.2.4",
    "@compodoc/compodoc": "^1.1.19",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "copy-files-from-to": "^3.2.2",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "netlify-cli": "^11.5.1",
    "ng-packagr": "^14.2.1",
    "typescript": "~4.6.4"
  },
  "copyFiles": [
    {
      "from": "README.md",
      "to": "dist/ngx-observers/README.md"
    }
  ],
  "compodoc": {
    "disableDependencies": true,
    "disableSourceCode": true,
    "hideGenerator": true,
    "theme": "material",
    "name": "Ngx observers"
  }
}
